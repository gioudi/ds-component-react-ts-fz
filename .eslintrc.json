{
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:storybook/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "@typescript-eslint",
    "react",
    "react-hooks",
    "storybook"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error"
  }
}
